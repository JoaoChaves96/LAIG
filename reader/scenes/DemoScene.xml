<dsx>

  <scene root="root" axis_length="3" />

  <views default="view1">

    <perspective id="view1" near="0.1" far="300" angle="40">
      <from x="15" y="15" z="15" />
      <to x="0" y="0" z="0" />
    </perspective>

    <perspective id="view2" near="0.1" far="500" angle="40">
      <from x="15" y="20" z="35" />
      <to x="5" y="5" z="5" />
    </perspective>

    <perspective id="view3" near="0.1" far="1500" angle="60">
        <from x="25" y="25" z="25" />
        <to x="0" y="0" z="0" />
    </perspective>

  </views>

  <illumination doublesided="1" local="1">

    <ambient r="0" g="0" b="0" a="1" />
    <background r="0" g="0" b="0" a="1" />

  </illumination>

  <lights>

    <omni id="omni1" enabled="1">
      <location x="4" y="6" z="1" w="1" />
      <ambient r="0" g="0" b="0" a="1" />
      <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
      <specular r="1.0" g="1.0" b="1.0" a="1.0" />
    </omni>

    <omni id="omni2" enabled="1">
      <location x="10.5" y="6.0" z="1.0" w="1.0" />
      <ambient r="0" g="0" b="0" a="1" />
      <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
      <specular r="1.0" g="1.0" b="1.0" a="1.0" />
    </omni>

    <omni id="omni3" enabled="1">
      <location x="10.5" y="6.0" z="15.0" w="1.0" />
      <ambient r="0" g="0" b="0" a="1" />
      <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
      <specular r="1.0" g="1.0" b="1.0" a="1.0" />
    </omni>

    <spot id="spot1" enabled="1" angle="40" exponent="0.5">
      <target x="1" y="1" z="0.5" />
      <location x="4" y="2" z="2" />
      <ambient r="0.5" g="0.5" b="0.5" a="0.5" />
      <diffuse r="0.5" g="0.5" b="0.5" a="0.5" />
      <specular r="0.5" g="0.5" b="0.5" a="0.5" />
    </spot>

  </lights>

  <textures>
    <texture id="floor" file="./resources/images/floor.png" length_s="10" length_t="10" />
    <texture id="chair" file="./resources/images/wood.jpg" length_s="1.0" length_t="1.0" />
    <texture id="table" file="./resources/images/marmore.jpg" length_s="1.0" length_t="1.0" />
    <texture id="buoy" file="./resources/images/buoy.jpg" length_s="1.0" length_t="1.0" />
    <texture id="towel" file="./resources/images/towel.jpg" length_s="2.0" length_t="2.0" />
    <texture id="tent" file="./resources/images/tent.jpg" length_s="1.0" length_t="1.0" />
    <texture id="ball" file="./resources/images/ball.png" length_s="1.0" length_t="1.0" />
    <texture id="sand" file="./resources/images/sand.jpg" length_s="10" length_t="10" />

  </textures>

  <materials>

    <material id="floor">
      <emission r="0.2" g="0.2" b="0.2" a="0.2" />
      <ambient r="0.3" g="0.3" b="0.3" a="1" />
      <diffuse r="1" g="1" b="1" a="1" />
      <specular r="0.2" g="0.2" b="0.2" a="1" />
      <shininess value="100" />
    </material>

    <material id="matA">
      <emission r="1" g="0.2" b="0.2" a="0.2" />
      <ambient r="1" g="0.3" b="0.3" a="1" />
      <diffuse r="1" g="0.2" b="0.2" a="0.5" />
      <specular r="1" g="0.2" b="0.2" a="1" />
      <shininess value="100" />
    </material>

    <material id="matB">
      <emission r="0.2" g="0.2" b="1" a="0.2" />
      <ambient r="0.2" g="0.3" b="1" a="1" />
      <diffuse r="0.2" g="0" b="1" a="0.5" />
      <specular r="0.2" g="0.2" b="1" a="1" />
      <shininess value="100" />
    </material>

    <material id="matC">
      <emission r="0.2" g="1" b="0.2" a="0.2" />
      <ambient r="0.3" g="1" b="0.3" a="1" />
      <diffuse r="0.1" g="1" b="0.1" a="0.5" />
      <specular r="1" g="1" b="1" a="1" />
      <shininess value="100" />
    </material>

  </materials>

  <transformations>

    <transformation id="root">
      <scale x="3" y="3" z="3" />
    </transformation>

  </transformations>

  <animations>
    <animation id="patchanim" span="3" type="linear">
      <controlpoint xx="0" yy="0" zz="0" />
      <controlpoint xx="5" yy="0" zz="0" />
      <controlpoint xx="5" yy="5" zz="0" />
    </animation>

    <animation id="patchanim2" span="3" type="circular"
      centerx="5" centery="5" centerz="0"
      radius="0" startang="0" rotang="90" />

  </animations>

  <primitives>

    <primitive id="rect1">
      <rectangle x1="0" y1="0" x2="1" y2="1" />
    </primitive>

    <primitive id="floor_rect">
      <rectangle x1="0" y1="0" x2="5" y2="5" />
    </primitive>

    <primitive id="triang1">
      <triangle x1="-0.6" y1="0" z1="0.6" x2="0.6" y2="0" z2="0.6" x3="0" y3="0.6" z3="0" />
    </primitive>

    <primitive id="sphere1">
      <sphere radius="0.3" slices="40" stacks="40" />
    </primitive>

    <primitive id="cylinder1">
      <cylinder base="1" top="1" height="1" slices="50" stacks="20" />
    </primitive>

    <primitive id="torus1">
      <torus inner="0.5" outer="1" slices="20" loops="20" />
    </primitive>

    <primitive id="plane1">
      <plane dimX="1" dimY="1" partsX="5" partsY="5" />
    </primitive>

    <primitive id="patch1">
      <patch orderU="2" orderV="1" partsU="10" partsV="10">
        <controlpoint x="-1.5" y="-1.5" z="0" />
        <controlpoint x="-1.5" y="1.5" z="0" />
        <controlpoint x="0" y="-1.5" z="3" />
        <controlpoint x="0" y="1.5" z="3" />
        <controlpoint x="1.5" y="-1.5" z="0" />
        <controlpoint x="1.5" y="1.5" z="0" />
      </patch>
    </primitive>

    <primitive id="patch2">
      <patch orderU="2" orderV="3" partsU="10" partsV="10">
        <controlpoint x="-1.5" y="-1.5" z="0.5" />
        <controlpoint x="-2.0" y="-2.0" z="0.5" />
        <controlpoint x="-2.0" y="-2.0" z="0.5" />
        <controlpoint x="-1.5" y="1.5" z="0.5" />
        <controlpoint x="0" y="0" z="3.0" />
        <controlpoint x="0" y="-2" z="3.0" />
        <controlpoint x="0" y="-2" z="3.0" />
        <controlpoint x="0" y="0" z="3.0" />
        <controlpoint x="1.5" y="-1.5" z="0.5" />
        <controlpoint x="2.0" y="-2" z="0.5" />
        <controlpoint x="2" y="2" z="0.5" />
        <controlpoint x="1.5" y="1.5" z="0.5" />
      </patch>
    </primitive>

  </primitives>

  <components>

    <component id="root">

      <transformation>
        <!--transformationref id="root" /-->
      </transformation>

      <materials>
        <material id="matA" />
      </materials>

      <texture id="none" />

      <children>
        <componentref id="floor" />
        <componentref id="buoy" />
        <componentref id="towel" />
        <componentref id="tent" />
        <componentref id="football" />
        <!--componentref id="patch" /-->
        <componentref id="parachute" />
      </children>

    </component>

    <component id="cube">

      <transformation>
        <scale x="1" y="1" z="1" />
      </transformation>

      <materials>
        <material id="inherit" />
      </materials>

      <texture id="inherit" />

      <children>
        <componentref id="front_r" />
        <componentref id="back_r" />
        <componentref id="top_r" />
        <componentref id="bottom_r" />
        <componentref id="right_r" />
        <componentref id="left_r" />
      </children>

    </component>

    <component id="front_r">

      <transformation>
        <translate x="-0.5" y="-0.5" z="0.5" />
      </transformation>

      <materials>
        <material id="inherit" />
      </materials>

      <texture id="towel" />

      <children>
        <primitiveref id="rect1"/>
      </children>

    </component>

    <component id="back_r">

      <transformation>
        <translate x="-0.5" y="0.5" z="-0.5" />
        <rotate axis="x" angle="180" />
      </transformation>

      <materials>
        <material id="inherit" />
      </materials>

      <texture id="inherit" />

      <children>
        <primitiveref id="rect1"/>
      </children>

    </component>

    <component id="top_r">

      <transformation>
        <translate x="0" y="0.5" z="0" />
        <rotate axis="x" angle="-90" />
        <translate x="-0.5" y="-0.5" z="0" />
      </transformation>

      <materials>
        <material id="inherit" />
      </materials>

      <texture id="inherit" />

      <children>
        <primitiveref id="rect1"/>
      </children>

    </component>

    <component id="bottom_r">

      <transformation>
        <translate x="0" y="-0.5" z="0" />
        <rotate axis="x" angle="90" />
        <translate x="-0.5" y="-0.5" z="0" />
      </transformation>

      <materials>
        <material id="inherit" />
      </materials>

      <texture id="inherit" />

      <children>
        <primitiveref id="rect1"/>
      </children>

    </component>

    <component id="right_r">

      <transformation>
        <translate x="0.5" y="-0.5" z="0.5" />
        <rotate axis="y" angle="90" />
      </transformation>

      <materials>
        <material id="inherit" />
      </materials>

      <texture id="inherit" />

      <children>
        <primitiveref id="rect1"/>
      </children>

    </component>

    <component id="left_r">

      <transformation>
        <translate x="-0.5" y="-0.5" z="-0.5" />
        <rotate axis="y" angle="-90" />
      </transformation>

      <materials>
        <material id="inherit" />
      </materials>

      <texture id="inherit" />

      <children>
        <primitiveref id="rect1"/>
      </children>

    </component>

    <component id="floor">

      <transformation>
        <scale x="5" y="0" z="5" />
        <translate x="0" y="0" z="1" />
        <rotate axis="x" angle="-90" />
      </transformation>

      <materials>
        <material id="floor" />
        <material id="matA" />
        <material id="matC" />
      </materials>

      <texture id="sand" />

      <children>
        <primitiveref id="rect1" />
      </children>

    </component>

    <component id="buoy">

      <transformation>
        <scale x="0.5" y="0.5" z="0.5" />
        <translate x="3" y="0.5" z="7" />
        <rotate axis="x" angle="-90" />
      </transformation>

      <materials>
        <material id="inherit" />
      </materials>

      <texture id="buoy" />

      <children>
        <primitiveref id="torus1" />
      </children>

    </component>

    <component id="towel">

      <transformation>
        <scale x="0.8" y="1" z="1.7" />
        <translate x="1.2" y="0.01" z="1.25" />
        <rotate axis="x" angle="-90" />
      </transformation>

      <materials>
        <material id="inherit" />
      </materials>

      <texture id="towel" />

      <children>
        <primitiveref id="rect1" />
      </children>

    </component>

    <component id="tent">

      <transformation>
        <translate x="4" y="0.6" z="3" />
      </transformation>

      <materials>
        <material id="inherit" />
      </materials>

      <texture id="tent" />

      <children>
        <componentref id="ceiling1" />
        <componentref id="ceiling2" />
        <componentref id="ceiling3" />
        <componentref id="ceiling4" />
        <componentref id="cubetent" />
        <componentref id="doortent" />
      </children>

    </component>

    <component id="ceiling1">

    <transformation>
      <translate x="0" y="0.6" z="0"/>
    </transformation>

    <materials>
      <material id="inherit" />
    </materials>

    <texture id="none" />

    <children>
      <primitiveref id="triang1"/>
    </children>

  </component>

  <component id="ceiling2">

  <transformation>
    <translate x="0" y="0.6" z="0"/>
    <rotate axis="y" angle="90" />
  </transformation>

  <materials>
    <material id="inherit" />
  </materials>

  <texture id="none" />

  <children>
    <primitiveref id="triang1"/>
  </children>

</component>

<component id="ceiling3">

<transformation>
  <translate x="0" y="0.6" z="0"/>
  <rotate axis="y" angle="180" />
</transformation>

<materials>
  <material id="inherit" />
</materials>

<texture id="none" />

<children>
  <primitiveref id="triang1"/>
</children>

</component>

<component id="ceiling4">

<transformation>
  <translate x="0" y="0.6" z="0"/>
  <rotate axis="y" angle="270" />
</transformation>

<materials>
    <material id="inherit" />
</materials>

<texture id="none" />

<children>
  <primitiveref id="triang1"/>
</children>

</component>

<component id="cubetent">

<transformation>
  <scale x="1.2" y="1.2" z="1.2"/>
</transformation>

<materials>
  <material id="inherit" />
</materials>

<texture id="inherit" />

<children>
  <componentref id="cube"/>
</children>

</component>

<component id="doortent">

<transformation>
  <translate x="-0.2" y="-0.601" z="0.601"/>
  <scale x="0.4" y="1" z="0"/>
</transformation>

<materials>
  <material id="inherit" />
</materials>

<texture id="chair" />

<children>
  <primitiveref id="rect1"/>
</children>

</component>

<component id="football">

  <transformation>
    <translate x="3" y="0.3" z="3"/>
  </transformation>

  <materials>
    <material id="inherit" />
  </materials>

  <texture id="ball" />

  <children>
    <primitiveref id="sphere1"/>
  </children>

</component>

<component id="patch">

  <transformation>
    <!--translate x="3" y="0.3" z="3"/-->
  </transformation>

  <animation>
    <!--animationref id="patchanim" />
    <animationref id="patchanim2"/-->
  </animation>

  <materials>
    <material id="inherit" />
  </materials>

  <texture id="towel" />

  <children>
    <primitiveref id="patch1"/>
  </children>

</component>

<component id="parachute">

  <transformation>
  </transformation>

  <materials>
    <material id="inherit" />
  </materials>

  <texture id="towel" />

  <children>
    <componentref id="parachute_top"/>
    <componentref id="parachute_cube"/>
    <componentref id="string1"/>
    <componentref id="string2"/>
    <componentref id="string3"/>
    <componentref id="string4"/>
  </children>

</component>

<component id="parachute_top">

  <transformation>
    <!--translate x="3" y="0.3" z="3"/-->
    <translate x="1" y="2" z="0.5" />
    <scale x="0.5" y="0.5" z="0.5" />
    <rotate axis="x" angle="-90" />
  </transformation>

  <materials>
    <material id="inherit" />
  </materials>

  <texture id="towel" />

  <children>
    <primitiveref id="patch2"/>
  </children>

</component>

<component id="parachute_cube">

  <transformation>
    <translate x="1" y="1" z="0.75" />
    <scale x="0.5" y="0.5" z="0.5" />
  </transformation>

  <materials>
    <material id="inherit" />
  </materials>

  <texture id="towel" />

  <children>
    <componentref id="cube"/>
  </children>

</component>

<component id="string1">

  <transformation>
    <rotate axis="z" angle="30" />
    <translate x="1.25" y="0.65" z="1"/>
    <scale x="0.05" y="1.2" z="1"/>

    <!--rotate axis="z" angle="35" /-->
  </transformation>

  <materials>
    <material id="inherit" />
  </materials>

  <texture id="towel" />

  <children>
    <primitiveref id="rect1"/>
  </children>

  </component>

  <component id="string2">

    <transformation>
      <rotate axis="z" angle="30" />
      <translate x="1.25" y="0.65" z="0.5"/>
      <scale x="0.05" y="1.2" z="1"/>

      <!--rotate axis="z" angle="35" /-->
    </transformation>

    <materials>
      <material id="inherit" />
    </materials>

    <texture id="towel" />

    <children>
      <primitiveref id="rect1"/>
    </children>

    </component>

    <component id="string3">

      <transformation>
        <rotate axis="z" angle="-30" />
        <translate x="0.45" y="1.65" z="1"/>
        <scale x="0.05" y="1.2" z="1"/>
      </transformation>

      <materials>
        <material id="inherit" />
      </materials>

      <texture id="towel" />

      <children>
        <primitiveref id="rect1"/>
      </children>

      </component>

    <component id="string4">

        <transformation>
          <rotate axis="z" angle="-30" />
          <translate x="0.45" y="1.65" z="0.5"/>
          <scale x="0.05" y="1.2" z="1"/>
        </transformation>

        <materials>
          <material id="inherit" />
        </materials>

        <texture id="towel" />

        <children>
          <primitiveref id="rect1"/>
        </children>

        </component>

</components>

</dsx>
