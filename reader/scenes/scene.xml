<dsx>

    <globals background="0 1 0 1" drawmode="fill" shading="gouraud" cullface="none" cullorder="CCW" />

	<list>
		<elem id="rect1" coords="0 0 1" />
	</list>

    <illumination>

        <ambient r="0.5" g="0.5" b="0.5" a="0.5" />
        <background r="0.0" g="0.0" b="0.0" a="1.0" />

    </illumination>

    <primitives>

        <primitive id="rect1">
            <rectangle x1="0" y1="0" x2="5" y2="5" />
        </primitive>

    </primitives>



    <components>

        <component id="cube">

            <transformation>

                <!--transformationref id="transf1" /-->

            </transformation>

            <!--materials>
                <material id="ss" />
            </materials-->

            <children>
                <componentref id="front_R" />
                <componentref id="back_r" />
                <componentref id="top_r" />
                <componentref id="bottom_r" />
                <componentref id="right_r" />
                <componentref id="left_r" />
            </children>

        </component>

        <component id="front_r">

          <transformations>
              <translate x="0" y="0" z="0.5" />
          </transformations>

          <children>
            <primitiveref id="rect1"/>
          </children>

        </component>

        <component id="back_r">

          <transformations>
              <rotate axis="x" angle="180" />
              <translate x="0" y="0" z="0.5" />
          </transformations>

          <children>
            <primitiveref id="rect1"/>
          </children>

        </component>

        <component id="top_r">

          <transformations>
            <rotate axis="x" angle="-90" />
            <translate x="0" y="0" z="0.5" />
          </transformations>

          <children>
            <primitiveref id="rect1"/>
          </children>

        </component>

        <component id="bottom_r">

          <transformations>
            <rotate axis="x" angle="90" />
            <translate x="0" y="0" z="0.5" />
          </transformations>

          <children>
            <primitiveref id="rect1"/>
          </children>

        </component>

        <component id="right_r">

          <transformations>
            <rotate axis="y" angle="-90" />
            <translate x="0" y="0" z="0.5" />
          </transformations>

          <children>
            <primitiveref id="rect1"/>
          </children>

        </component>

        <component id="left_r">

          <transformations>
              <rotate axis="y" angle="90" />
              <translate x="0" y="0" z="0.5" />
          </transformations>

          <children>
            <primitiveref id="rect1"/>
          </children>

        </component>

        <component id="chair">

          <transformations>
            <translate x="-0.5" y="0.125" z="-0.5" />
          </transformations>

          <children>
            <componentref id="supp1" />
            <componentref id="supp2" />
            <componentref id="back-rest" />
            <componentref id="sit" />
            <componentref id="leg1" />
            <componentref id="leg2" />
            <componentref id="leg3" />
            <componentref id="leg4" />
          </children>

        </component>

        <component id="supp1">

          <transformations>
            <translate x="0" y="1" z="0" />
            <scale x="0.25" y="2" z="0.25" />
          </transformations>

          <children>
            <componentref id="cube" />
          </children>

        </component>

        <component id="supp2">

          <transformations>
            <translate x="0" y="1" z="1" />
            <scale x="0.25" y="2" z="0.25" />
          </transformations>

          <children>
            <componentref id="cube" />
          </children>

        </component>

        <component id="back-rest">

          <transformations>
            <scale x="1.25" y="0.25" z="1.25" />
            <translate x="0.4" y="-0.5" z="0.4" />
          </transformations>

          <children>
            <componentref id="cube" />
          </children>

        </component>

        <component id="leg1">

          <transformations>
            <translate x="0" y="-0.8" z="0" />
            <scale x="0.25" y="1.2" z="0.25" />
          </transformations>

          <children>
            <componentref id="cube" />
          </children>

        </component>

        <component id="leg2">

          <transformations>
            <translate x="0" y="-0.8" z="1" />
            <scale x="0.25" y="1.2" z="0.25" />
          </transformations>

          <children>
            <componentref id="cube" />
          </children>

        </component>

        <component id="leg3">

          <transformations>
            <translate x="1" y="-0.8" z="0" />
            <scale x="0.25" y="1.2" z="0.25" />
          </transformations>

          <children>
            <componentref id="cube" />
          </children>

        </component>

        <component id="leg4">

          <transformations>
            <translate x="1" y="-0.8" z="1" />
            <scale x="0.25" y="1.2" z="0.25" />
          </transformations>

          <children>
            <componentref id="cube" />
          </children>

        </component>

        <component id="desk">

          <transformations>

          </transformations>

          <children>
            <componentref id="desktop" />
            <componentref id="deskleg1" />
            <componentref id="deskleg2" />
            <componentref id="deskleg3" />
            <componentref id="deskleg4" />
          </children>

        </component>

        <component id="desktop">

          <transformations>
              <translate x="0" y="1" z="0" />
              <scale x="3" y="0.125" z="1.5">
          </transformations>

          <children>
            <componentref id="cube"/>
          </children>

        </component>

        <component id="deskleg1">

          <transformations>
              <rotate axis="x" angle="90" />
              <translate x="1.2" y="0.5" z="0" />
              <scale x="0.1" y="0.1" z="2" />
          </transformations>

          <children>
            <primitiveref id="cylinder"/>
          </children>

        </component>

        <component id="deskleg2">

          <transformations>
              <rotate axis="x" angle="90" />
              <translate x="1.2" y="-0.5" z="0" />
              <scale x="0.1" y="0.1" z="2" />
          </transformations>

          <children>
            <primitiveref id="cylinder"/>
          </children>

        </component>

        <component id="deskleg3">

          <transformations>
              <rotate axis="x" angle="90" />
              <translate x="-1.2" y="0.5" z="0" />
              <scale x=0.1" y="0.1 z="2" />
          </transformations>

          <children>
            <primitiveref id="cylinder"/>
          </children>

        </component>

        <component id="deskleg1">

          <transformations>
              <rotate axis="x" angle="90" />
              <translate x="-1.2" y="-0.5" z="0" />
              <scale x="0.1" y="0.1" z="2" />
          </transformations>

          <children>
            <primitiveref id="cylinder"/>
          </children>

        </component>

    </components>

</dsx>
